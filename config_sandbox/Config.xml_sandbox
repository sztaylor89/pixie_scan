  <Description>
    Used for comparing Beta energy and timing signals.
  </Description>
  
  <Global>
    <Revision version="F"/>
    <EventWidth unit="s" value="1e-6"/>
    <EnergyContraction value="1.0"/>
    <Path>config/</Path>
    <NumOfTraces value="50"/>
